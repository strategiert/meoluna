{
  "version": 2,
  "builds": [
    {
      "src": "apps/platform/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/platform/$1"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "cd apps/platform && npm run build"
}